<AnchorPane id="AnchorPane" fx:id="ap" prefHeight="600.0" prefWidth="725.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.BlogListController">
   <children>
      <RadioButton fx:id="rd_my" layoutX="512.0" layoutY="136.0" mnemonicParsing="false" onAction="#rd_my" text="Only my blogs" />
      <Label layoutX="51.0" layoutY="36.0" text="Blogs">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TableView fx:id="tv" layoutX="51.0" layoutY="126.0" onMouseClicked="#doubleClick" prefHeight="318.0" prefWidth="610.0">
        <columns>
          <TableColumn fx:id="title" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="contenu" prefWidth="299.20006561279297" text="Contenu" />
            <TableColumn fx:id="date" prefWidth="139.99996948242188" text="Date" />
            <TableColumn fx:id="statut" prefWidth="97.60003662109375" text="Statut" />
        </columns>
      </TableView>
      <TextField fx:id="search" layoutX="51.0" layoutY="83.0" onKeyPressed="#search" prefHeight="26.0" prefWidth="307.0" />
      <Button fx:id="ajouter" layoutX="537.0" layoutY="465.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="26.0" prefWidth="124.0" text="Ajouter Blog" />
   </children>
</AnchorPane>
